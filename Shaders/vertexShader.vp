#version 400 core

in vec3 position;
in vec3 normal;
in vec2 texCoord;

out vec2 pass_textureCoords;

uniform mat4 TRS;
uniform mat4 Projection;
uniform mat4 View;
uniform mat4 MVP;
uniform vec3 LightPosition;

void main(void){
	gl_Position = MVP* vec4(position,1.0);
	pass_textureCoords = texCoord;

}